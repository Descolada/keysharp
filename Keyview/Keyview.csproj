<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>Keyview.Program</StartupObject>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <ApplicationIcon>..\Keysharp.ico</ApplicationIcon>
    <AssemblyVersion>0.0.0.7</AssemblyVersion>
    <FileVersion>0.0.0.7</FileVersion>
    <Version>0.0.0.7</Version>
    <Authors>Matt Feemster</Authors>
    <Description>A viewer which shows the C# code that is generated from compiling Keysharp script code</Description>
    <Company>Keysharp</Company>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
    <SignAssembly>False</SignAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\bin\debug\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\bin\release\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\Keysharp.ico" Link="Keysharp.ico" />
    <Content Include="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x64\Lexilla.dll" />
    <Content Include="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x64\Scintilla.dll" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Scintilla5.NET" Version="5.3.3.12" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Keysharp.Core\Keysharp.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x86\Lexilla.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x86\Scintilla.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Remove="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x64\Lexilla.dll" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="C:\Users\redoubter\.nuget\packages\scintilla5.net\5.3.3.12\build\x64\Scintilla.dll" />
  </ItemGroup>

</Project>